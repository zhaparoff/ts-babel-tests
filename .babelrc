{
    "presets": [
        [
            "@babel/preset-env",
            {
                "debug": true,
                "modules": false,
                "useBuiltIns": false,
                "corejs": "3.1.3",
                "targets": {
                    "ie": "11",
                    "edge": "17",
                    "chrome": "70"
                }
            }
        ],
        "@babel/preset-typescript"
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        [
            "@babel/plugin-transform-runtime",
            {
                "corejs": false,
                "helpers": false,
                "regenerator": false,
                "useESModules": false
            }
        ]
    ]
}